{
  "name": "movoin/one-example",
  "description": "Just one framework.",
  "type": "library",
  "license": "Apache-2.0",
  "homepage": "https://github.com/movoin/one-example",
  "authors": [
    {
      "name": "Allen Luo",
      "email": "movoin@gmail.com",
      "homepage": "https://github.com/movoin"
    }
  ],
  "bin": [
    "bin/one"
  ],
  "autoload": {
    "psr-4": {
      "One\\Example\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "One\\Example\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "ext-swoole": ">=4.2.0",
    "rybakit/msgpack": "^0.7.0",
    "symfony/console": "^5.0",
    "symfony/process": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpstan/phpstan": "^0.12.9",
    "phpunit/phpunit": "^7.5",
    "symfony/finder": "^5.0"
  },
  "scripts": {
    "test": [
      "phpstan analyze src --level max",
      "phpunit -c ./phpunit.xml"
    ],
    "phpunit": "phpunit -c ./phpunit.xml",
    "api-docs": "sami update ./sami.inc"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "secure-http": false
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}